{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "sku": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 150
    },
    "price": {
      "type": "number",
      "minimum": 1
    },
    "size": {
      "type": "integer",
      "minimum": 1
    },
    "type": {
      "type": "string",
      "minLength": 1,
      "maxLength": 40
    }
  },
  "required": [
    "sku",
    "name",
    "price",
    "size",
    "type"
  ]
}